{
    "name": "CurvatureWavefrontSensing",
    "program": "cwfs",
    "constraints": [],
    "scripts": [
        {
            "name": "auxtel/latiss_wep_align.py",
            "standard": false,
            "parameters": {
                "find_target": {
                    "az": "$az",
                    "el": "$alt",
                    "mag_limit": 6.0,
                    "mag_range": 4.0,
                    "radius": 5.0
                },
                "filter": "SDSSr_65mm",
                "program": "$program"
            }
        },
        {
            "name": "auxtel/latiss_intra_extra_focal_data.py",
            "standard": false,
            "parameters": {
                "program": "$program",
                "reason": "cwfs_corrected"
            }
        }
    ]
}